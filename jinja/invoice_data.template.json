{
    "documentType": "Invoice",
    "number": "{{ number | default(value=1) }}",
    "date": "{{ date | default(value="2024-01-01")}}",
    "vat": {{ vat | default(value=24) }},
    "sender": {{ env.INVOICE_SENDER }},
    "recipient": {{ env.INVOICE_RECIPIENT }},
    "items": {{ items | json_encode() | safe }},
    "amounts": {{ amounts | json_encode() | safe }}
}
